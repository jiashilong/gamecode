package com.game.server.packet;

import org.jboss.netty.buffer.ChannelBuffer;

public abstract class Request {
	
	private int id;
	
	public void setId(int id) {
		this.id = id;
	}
	
	public  int getId() {
		return id;
	}
	
	public void decodeBuffer(ChannelBuffer buffer) {
		
	}
}
